{
  "name": "multi-agent-system",
  "version": "1.0.0",
  "description": "Multi-agent system for GitHub intelligence, social media, lead generation, and blockchain IP management",
  "main": "dist/index.js",
  "scripts": {
    "dev:orchestrator": "tsx src/aws/index.ts",
    "dev:github": "tsx src/aws/servers/github.ts",
    "dev:email": "tsx src/aws/servers/email.ts",
    "dev:social": "tsx src/aws/servers/socials.ts",
    "dev:leads": "tsx src/aws/servers/leads.ts",
    "dev:blockchain": "tsx src/aws/servers/ip.ts",
    "dev:karma": "tsx src/aws/servers/karma.ts",
    "dev:monitoring": "tsx src/aws/servers/compliance.ts",
    "dev:all": "concurrently \"npm run dev:orchestrator\" \"npm run dev:github\" \"npm run dev:email\" \"npm run dev:social\" \"npm run dev:leads\" \"npm run dev:blockchain\" \"npm run dev:karma\" \"npm run dev:monitoring\"",
    "dev:webhook": "tsx src/aws/webhook-listener.ts",
    "dev:local": "tsx src/local/server.ts",
    "test": "tsx src/local/test.ts"
  },
  "keywords": [
    "multi-agent",
    "github-api",
    "social-media",
    "lead-generation",
    "blockchain",
    "aws",
    "microservices",
    "typescript"
  ],
  "author": "Gabriel Antony Xavier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/multi-agent-system.git"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.826.0",
    "@aws-sdk/client-cloudwatch": "^3.826.0",
    "@aws-sdk/client-dynamodb": "^3.826.0",
    "@aws-sdk/client-eventbridge": "^3.826.0",
    "@aws-sdk/client-kms": "^3.826.0",
    "@aws-sdk/client-lambda": "^3.826.0",
    "@aws-sdk/client-opensearch": "^3.826.0",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/client-ses": "^3.826.0",
    "@aws-sdk/client-sfn": "^3.826.0",
    "@aws-sdk/client-sns": "^3.826.0",
    "@aws-sdk/client-sqs": "^3.826.0",
    "@aws-sdk/util-dynamodb": "^3.826.0",
    "@octokit/rest": "^22.0.0",
    "@types/express": "^5.0.3",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  }
}
