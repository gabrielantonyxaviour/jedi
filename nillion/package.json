{
  "name": "nillion-multi-agent-system",
  "version": "1.0.0",
  "description": "Multi-agent system with Nillion SecretVault integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm run build && node dist/index.js",
    "start": "node dist/index.js",
    "test:logs": "pnpm run build && node dist/scripts/test-logs.js",
    "test:github": "pnpm run build && node dist/scripts/test-github.js",
    "test:leads": "pnpm run build && node dist/scripts/test-leads.js",
    "test:simple": "pnpm run build && node dist/scripts/test-simple.js",
    "test:stories": "pnpm run build && node dist/scripts/test-stories.js",
    "test:socials": "pnpm run build && node dist/scripts/test-socials.js",
    "test:grants": "pnpm run build && node dist/scripts/test-grants.js",
    "test:compliance": "pnpm run build && node dist/scripts/test-compliance.js",
    "test:all": "pnpm run build && NODE_ENV=test node dist/scripts/test-logs.js 2>/dev/null && node dist/scripts/test-github.js 2>/dev/null && node dist/scripts/test-leads.js 2>/dev/null && node dist/scripts/test-stories.js 2>/dev/null && node dist/scripts/test-socials.js 2>/dev/null && node dist/scripts/test-grants.js 2>/dev/null && node dist/scripts/test-compliance.js 2>/dev/null",
    "analytics": "pnpm run build && node dist/scripts/analytics.js",
    "setup:schemas": "pnpm run build && node dist/scripts/setup-schemas.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@nillion/nilql": "latest",
    "dotenv": "^16.0.0",
    "mongodb": "^6.0.0",
    "secretvaults": "latest",
    "tsx": "^4.20.3",
    "uuid": "^9.0.0"
  }
}
