{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Agent Logs Collection",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid",
        "coerce": true
      },
      "projectId": {
        "type": "string",
        "format": "uuid"
      },
      "agentName": {
        "type": "string",
        "enum": [
          "github",
          "leads",
          "socials",
          "karma",
          "ip",
          "compliance",
          "orchestrator"
        ]
      },
      "text": {
        "type": "string"
      },
      "data": {
        "type": "object"
      }
    },
    "required": ["_id", "projectId", "agentName", "text", "data"],
    "additionalProperties": false
  }
}
