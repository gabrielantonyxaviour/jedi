{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Compliance Collection",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid",
        "coerce": true
      },
      "name": {
        "type": "string"
      },
      "metadata": {
        "type": "object"
      }
    },
    "required": ["_id", "name", "metadata"],
    "additionalProperties": false
  }
}
